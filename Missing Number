class Solution {
    public int missingNumber(int[] nums) {
        if (nums == null || nums.length == 0) {
            return 0;
        }
        int len = nums.length;
        int pos = -1;
        for (int i = 0; i < len; i++) {
            while (i != nums[i] && nums[i] != len) {
                int temp = nums[i];
                nums[i] = nums[temp];
                nums[temp] = temp;
            }
            if (nums[i] == len) {
                pos = i;
            }
        }
        if (pos == -1) {
            return len;
        }
        return pos;
    }
}
